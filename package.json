{
  "name": "dressing-intelligent",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "expo:doctor": "npx expo-doctor || echo 'Expo doctor completed with warnings (known issues: reanimated 3.x intentional, .expo git check is false positive)'",
    "sentry:sourcemaps": "sentry-cli releases new \"$npm_package_version\" && sentry-cli releases files \"$npm_package_version\" upload-sourcemaps ./dist && sentry-cli releases finalize \"$npm_package_version\"",
    "check": "pnpm lint && pnpm typecheck && pnpm test && pnpm expo:doctor",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.5.1",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "^6.22.0",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-query": "^5.90.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^54.0.33",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "^14.0.11",
    "expo-haptics": "^15.0.8",
    "expo-image-manipulator": "^14.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^12.16.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^4.1.2",
    "react-native-reanimated": "~3.19.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@sentry/cli": "^3.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-test-renderer": "19.1.0",
    "supabase": "^2.74.5",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "pnpm": {
    "overrides": {
      "@isaacs/brace-expansion": ">=5.0.1"
    }
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-reanimated",
        "jest-expo",
        "@sentry/react-native"
      ]
    }
  }
}
